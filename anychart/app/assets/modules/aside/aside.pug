//- style
  include aside.css

#aside.tab-area
  h5#aside-header Oбзор рынка
  button#aside-close-btn &times;

  .tab-headers-wrapper
    .tab-header.tab-header_active(data-tab='active') Активная пара
    .tab-header(data-tab='all') Все пары

  .tab-bodies-wrapper
    .tab-body.tab-body_aside.tab-body_active(data-tab='active' style='overflow: auto')
      .active-pair-wrapper
        .active-pair
          .active-pair__name BTCETH
          .active-pair__asc
            span ASC
            .active-pair__asc-value(style='color:blue') 0.90404
          .active-pair__bid
            span BID
            .active-pair__bid-value(style='color:red') 0.904027

        .order-placing
          //- h5.order-placing__header РАЗМЕСТИТЬ СДЕЛКУ
          .order-placing__item
            .order-placing__name 0.1 лот
            .order-placing__value $ 184.54
          .order-placing__item
            .order-placing__name Цена пункта:
            .order-placing__value $ 0.10
          .order-placing__item
            .order-placing__name Необходимая маржа:
            .order-placing__value $ 6.88
          .order-placing__item
            .order-placing__name Спред:
            .order-placing__value -
          .order-placing__item
            .order-placing__name Левередж:
            .order-placing__value 1:100
          .order-placing__item
            .order-placing__name Своп при покупке (1 лот):
            .order-placing__value -3.47
          .order-placing__item
            .order-placing__name Своп при продаже (1 лот):
            .order-placing__value -2.08
          .order-placing__item
            .order-placing__name Минимальное изменение:
            .order-placing__value 0.00010

          .order-placing__input-item
            .order-placing__input-description Stop Lose:
            .order-placing__input-wrapper
              input.order-placing__input#stopLose(type="text" value="0" data-minvalue='0' data-maxvalue='9999.99999' data-step='0.01' data-validate='numeric')
              button.order-placing__input-button(data-behavior='increment')
                svg(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"): path(d="M288.662 352H31.338c-17.818 0-26.741-21.543-14.142-34.142l128.662-128.662c7.81-7.81 20.474-7.81 28.284 0l128.662 128.662c12.6 12.599 3.676 34.142-14.142 34.142z")
              button.order-placing__input-button(data-behavior='decrement')
                svg(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"): path(d="M31.3 192h257.3c17.8 0 26.7 21.5 14.1 34.1L174.1 354.8c-7.8 7.8-20.5 7.8-28.3 0L17.2 226.1C4.6 213.5 13.5 192 31.3 192z")

          .order-placing__input-item
            .order-placing__input-description Take Profit:
            .order-placing__input-wrapper
              input.order-placing__input#takeProfit(type="text" value="0" data-minvalue='0' data-maxvalue='9999.99999' data-step='0.01' data-validate='numeric')
              button.order-placing__input-button(data-behavior='increment')
                svg(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"): path(d="M288.662 352H31.338c-17.818 0-26.741-21.543-14.142-34.142l128.662-128.662c7.81-7.81 20.474-7.81 28.284 0l128.662 128.662c12.6 12.599 3.676 34.142-14.142 34.142z")
